<?php
 for ($i = 1; $i <= 50; $i++) {
     $a = 0;
     for ($j = 1; $j <= $i; $j++) { 
         if ($i % $j == 0) {
             $a++;
         }
     }
     if ($a == 2) {
     echo "$i, ";
     }
 }
// versi pertama..

echo "<hr>";

$a = 2;
while ($a <= 50) {
    if ($a == 2 || $a == 3 || $a == 5 || $a == 7) {
        echo "$a. <br>";
    } elseif ($a % 2 != 0 && $a % 3 != 0 && $a % 5 != 0 && $a % 7 != 0) {
        echo "$a. <br>";
    }
    $a++;
}
// versi kedua..
